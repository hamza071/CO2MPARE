<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.co2mpare.InlogController">
  <TextField fx:id="usernameField" promptText="Gebruikersnaam" />
  <PasswordField fx:id="passwordField" promptText="Wachtwoord" />
  <Button fx:id="loginButton" text="Inloggen" onAction="#handleLogin"/>
</StackPane>
